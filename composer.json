{
  "name": "rudloff/openvegemap-editor",
  "description": "Edit tags on OpenVegeMap",
  "license": "GPL-3.0-only",
  "require": {
    "php": ">=7.3",
    "guzzlehttp/guzzle": "~6.3.3",
    "jmikola/geojson": "~1.0.2",
    "slim/slim": "^3.5",
    "mathmarques/smarty-view": "~1.1.1",
    "servo/fluidxml": "~1.21.0",
    "plasticbrain/php-flash-messages": "~1.0.1",
    "maxh/php-nominatim": "^2.3",
    "composer/package-versions-deprecated": "1.11.99.5"
  },
  "require-dev": {
    "symfony/var-dumper": "~3.1.0",
    "squizlabs/php_codesniffer": "^3.5",
    "phpunit/phpunit": "^9.5",
    "insite/composer-dangling-locked-deps": "^0.2.1"
  },
  "autoload": {
    "psr-4": {
      "OpenVegeMap\\Editor\\": "classes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OpenVegeMap\\Test\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "insite/composer-dangling-locked-deps": true
    }
  }
}
